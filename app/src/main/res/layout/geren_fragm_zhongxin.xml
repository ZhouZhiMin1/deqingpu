            android:background="#22323232"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="89px"
            android:background="@color/white"
            android:paddingLeft="32px"
            android:paddingRight="32px">
            <ImageView android:id="@+id/iv_gr_dianpu"
                android:layout_width="35px"
                android:layout_height="36px"
                android:scaleType="fitXY"
                android:src="@drawable/icon_73"
                android:layout_centerVertical="true"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="67px"
                android:layout_marginLeft="34px"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/iv_gr_dianpu">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="我的店铺"
                    android:layout_centerVertical="true"
                    android:textColor="@color/zilanmu_textcolor"
                    android:textSize="28px"
                    android:layout_alignParentLeft="true"/>
                <ImageView
                    android:layout_width="13px"
                    android:layout_height="25px"
                    android:src="@drawable/icon_58"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"/>
            </RelativeLayout>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="32px"
            android:layout_marginRight="32px"
            android:background="#22323232"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="89px"
            android:background="@color/white"
            android:paddingLeft="32px"
            android:paddingRight="32px">
            <ImageView android:id="@+id/iv_gr_chanpin"
                android:layout_width="35px"
                android:layout_height="36px"
                android:scaleType="fitXY"
                android:src="@drawable/icon_72"
                android:layout_centerVertical="true"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="67px"
                android:layout_marginLeft="34px"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/iv_gr_chanpin">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="产品入驻"
                    android:layout_centerVertical="true"
                    android:textColor="@color/zilanmu_textcolor"
