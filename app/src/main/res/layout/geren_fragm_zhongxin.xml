                    android:textColor="@color/zilanmu_textcolor"
                    android:textSize="28px"
                    android:layout_alignParentLeft="true"/>
                <ImageView
                    android:layout_width="13px"
                    android:layout_height="25px"
                    android:src="@drawable/icon_58"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"/>
            </RelativeLayout>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="32px"
            android:layout_marginRight="32px"
            android:background="#22323232"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="89px"
            android:paddingLeft="32px"
            android:paddingRight="32px">

            <ImageView
                android:id="@+id/iv_gr_dizhi"
                android:layout_width="35px"
                android:layout_height="36px"
                android:layout_centerVertical="true"
                android:scaleType="fitXY"
                android:src="@drawable/icon_69" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="67px"
                android:layout_centerVertical="true"
                android:layout_marginLeft="34px"
                android:layout_toRightOf="@+id/iv_gr_dizhi">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="我的地址"
                    android:textColor="@color/zilanmu_textcolor"
                    android:textSize="28px" />

                <ImageView
                    android:layout_width="13px"
                    android:layout_height="25px"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/icon_58" />
            </RelativeLayout>
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="32px"
            android:layout_marginRight="32px"
            android:background="#22323232"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="89px"
            android:paddingLeft="32px"
            android:paddingRight="32px">
            <ImageView android:id="@+id/iv_gr_dindan"
                android:layout_width="35px"
                android:layout_height="36px"
                android:scaleType="fitXY"
                android:src="@drawable/icon_66"
                android:layout_centerVertical="true"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="67px"
                android:layout_marginLeft="34px"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/iv_gr_dindan">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="我的订单"
                    android:layout_centerVertical="true"
                    android:textColor="@color/zilanmu_textcolor"
                    android:textSize="28px"
                    android:layout_alignParentLeft="true"/>
                <ImageView
                    android:layout_width="13px"
                    android:layout_height="25px"
                    android:src="@drawable/icon_58"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"/>
            </RelativeLayout>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="32px"
            android:layout_marginRight="32px"
            android:background="#22323232"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="89px"
            android:paddingLeft="32px"
            android:paddingRight="32px">
            <ImageView android:id="@+id/iv_gr_gouwuche"
                android:layout_width="35px"
                android:layout_height="36px"
                android:scaleType="fitXY"
                android:src="@drawable/icon_74"
                android:layout_centerVertical="true"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="67px"
                android:layout_marginLeft="34px"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/iv_gr_gouwuche">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="我的购物车"
                    android:layout_centerVertical="true"
                    android:textColor="@color/zilanmu_textcolor"
                    android:textSize="28px"
                    android:layout_alignParentLeft="true"/>
                <ImageView
                    android:layout_width="13px"
                    android:layout_height="25px"
                    android:src="@drawable/icon_58"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"/>
            </RelativeLayout>
        </RelativeLayout>


    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#09323232"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="10px"
        android:background="#09323232"/>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="89px"
            android:background="@color/white"
            android:paddingLeft="32px"
            android:paddingRight="32px">
            <ImageView android:id="@+id/iv_gr_renzheng"
                android:layout_width="35px"
                android:layout_height="36px"
                android:scaleType="fitXY"
                android:src="@drawable/icon_70"
