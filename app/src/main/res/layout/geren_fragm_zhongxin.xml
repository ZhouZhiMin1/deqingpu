<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f9f9f9">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="118px"
        android:background="@color/titlecolor">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="个人中心"
            android:textColor="@color/white"
            android:textSize="35px"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="12px"/>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="13px"
        android:background="#09323232"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="118px"
        android:layout_marginTop="0px"
        android:background="@color/white"
        android:paddingLeft="32px"
        android:paddingRight="32px">
        <ImageView android:id="@+id/iv_gr_touxiang"
            android:layout_width="100px"
            android:layout_height="100px"
            android:scaleType="fitXY"
            android:src="@drawable/vatarsample346"
            android:layout_centerVertical="true"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="67px"
            android:layout_marginLeft="24px"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/iv_gr_touxiang">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="德卿铺"
                android:textColor="@color/titlebar"
                android:textSize="28px"
                android:layout_alignParentTop="true"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="客观号：110"
                android:textColor="@color/titlebar"
                android:textSize="24px"
                android:layout_alignParentBottom="true"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="进入个人主页 >"
                    android:textColor="@color/titlecolor"
                    android:textSize="24px"/>
                <ImageView
                    android:layout_width="30px"
                    android:layout_height="15px"
                    android:visibility="gone"
                    android:background="@drawable/icon_58"/>
            </LinearLayout>

        </RelativeLayout>

    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#09323232"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="13px"
        android:background="#09323232"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="89px"
        android:background="@color/white"
        android:paddingLeft="32px"
        android:paddingRight="32px">
        <ImageView android:id="@+id/iv_gr_quanzi"
            android:layout_width="35px"
            android:layout_height="36px"
            android:scaleType="fitXY"
            android:src="@drawable/icon_67"
            android:layout_centerVertical="true"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="67px"
            android:layout_marginLeft="34px"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/iv_gr_quanzi">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="圈子"
                android:layout_centerVertical="true"
                android:textColor="@color/zilanmu_textcolor"
                android:textSize="28px"
                android:layout_alignParentLeft="true"/>
            <ImageView
                android:layout_width="13px"
                android:layout_height="25px"
                android:src="@drawable/icon_58"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"/>
        </RelativeLayout>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#09323232"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="10px"
        android:background="#09323232"/>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="vertical">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="89px"

            android:paddingLeft="32px"
            android:paddingRight="32px">
